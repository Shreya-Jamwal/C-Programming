#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, first, last;
    scanf("%d", &num);
    if(num >= 0 && num <= 9)
        printf("%d", num);
    else{
        last = num%10;
        num = num/10;
        int count = 0, temp = num;
        while(temp != 0){
            temp = temp/10;
            count++;
        }
        first = num/pow(10, count-1);
        num = num%(int)pow(10, count-1);
        num = last*pow(10, count) + num*10 + first;
        printf("%d", num);
    }
    return 0;
}
