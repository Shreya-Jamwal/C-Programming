#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int U;
    float A, T, S;
    scanf("%d", &U);
    
    if (U<=50) A=U*0.50;
    else if (U<=150) A=25+((U-50)*0.75);
    else if (U<=250) A=100+((U-150)*1.20);
    else A = 220 + ((U-250)*1.50);
    
    S = A*0.20;
    T = S + A;
    printf("%.2f", T); 
    return 0;
}
